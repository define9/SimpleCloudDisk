<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.tomisme.mapper.ResourcesMapper">

    <select id="getIdByHashAndSize" resultType="integer">
        select `id` from `resources` where `hash`=#{hash} and `resources_size`=#{size}
    </select>
</mapper>
